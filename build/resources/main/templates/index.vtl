<h1>The Places You've Been!</h1>

<p>Add a new City:</p>

<form action="/cities" method="post">
  <label for="nameOfCity">City Name</label>
  <input id="nameOfCity" name="nameOfCity" type="text">

  <button type="submit" class="btn btn-default">Add City</button>
  <ul>
#foreach ($city in $cities)
  <li>$city.getnewCity()</li>
#end
</ul>
</form>
